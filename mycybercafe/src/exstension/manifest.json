{
    "name": "MyCyberCáfe",
    "version": "1.0.0",
    "description": "Screen time web browser extension",
    "manifest_version": 3,
    "author": "Jamie, Daniel, Christopher",
    "background" : {
      "service_worker" : "background.js"
    },
    "action": {
      "icon": {
        "32": "mug.jpg"
      },
      "default_popup": "index.html",
      "default_title": "MyCyberCáfe"
    },
    "permissions": [
      "tabs",
       "activeTab",
       "webRequest"
    ],

    "host_permissions": ["<all_urls>"],
    
      "content_scripts": [
        {
          "js": ["content.js"],
          "matches": ["<all_urls>"]
       
          
        }
      ]
  }
  